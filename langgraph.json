        ".",
        "langgraph==0.6.7",
        "langgraph-cli>=0.4.4",
        "langchain>=0.3.19",
        "langchain-experimental>=0.3.4",
        "langchain-openai>=0.3.7",
        "langchain-cohere==0.4.4",
        "cohere==5.12.0",
        "langchain-qdrant>=0.2.0",
        "qdrant-client>=1.13.2",
        "rank-bm25>=0.2.2",
        "pymupdf>=1.26.3",
